<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jioke.Arrows4Revival" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Arrows4Revival</name>
    <description>
        A digital year book for the final year 2016/2017 session of Anglican Student's Fellowship, Nnamdi Azikiwe University, Awka
    </description>
    <author email="okwuosachijioke@gmail.com">
        Excellence Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-dialogs" spec="D:\MY BOOKS\mobile app\cordova plugins\cordova-plugin-dialogs" />
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="portrait" />
    <preference name="SplashScreenDelay" value="4000" />
    <engine name="android" spec="^7.0.0" />
</widget>
